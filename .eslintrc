{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "react/prop-types": 0
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "parser": "babel-eslint"
}